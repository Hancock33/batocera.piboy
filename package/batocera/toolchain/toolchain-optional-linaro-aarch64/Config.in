config BR2_TOOLCHAIN_OPTIONAL_LINARO_AARCH64
	bool "toolchain-optional-linaro-aarch64"
	depends on BR2_HOSTARCH = "x86_64" || BR2_HOSTARCH = "x86"

	help
	  Toolchain for the AArch64 architecture that can be optionally used by packages at compile time, from
	  http://www.linaro.org/engineering/armv8/
