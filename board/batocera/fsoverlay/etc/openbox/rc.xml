<?xml version="1.0" encoding="UTF-8"?>
<openbox_config xmlns="http://openbox.org/3.4/rc" xmlns:xi="http://www.w3.org/2001/XInclude">
	<desktops>
		<number>1</number>
		<firstdesk>1</firstdesk>
	</desktops>
	<applications>
		<application class="*">
			<decor>no</decor>
			<maximized>true</maximized>
 			<focus>yes</focus>
			<layer>normal</layer>
			<fullscreen>yes</fullscreen>
		</application>
	</applications>
</openbox_config>
